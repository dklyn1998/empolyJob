<!-- 顶部用户信息栏 -->
<view calss='topmsg'>
  <image src='/image/background.png' class='bgimg'></image>
  <view class='usermsg'>
    <image src='{{usermsg.logo}}'></image>
    <text>{{usermsg.company_name}}</text>
  </view>
    <text class='mymoney' bindtap='invest'>余额充值</text>
